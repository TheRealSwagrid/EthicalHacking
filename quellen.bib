@misc{Wikipedia.2021d,
 abstract = {In the field of computer security, independent researchers often discover flaws in software that can be abused to cause unintended behaviour; these flaws are called vulnerabilities. The process by which the analysis of these vulnerabilities is shared with third parties is the subject of much debate, and is referred to as the researcher's disclosure policy. Full disclosure is the practice of publishing analysis of software vulnerabilities as early as possible, making the data accessible to everyone without restriction. The primary purpose of widely disseminating information about vulnerabilities is so that potential victims are as knowledgeable as those who attack them.In his 2007 essay on the topic, Bruce Schneier stated {\textquotedbl}Full disclosure -- the practice of making the details of security vulnerabilities public -- is a damned good idea. Public scrutiny is the only reliable way to improve security, while secrecy only makes us less secure{\textquotedbl}. Leonard Rose, co-creator of an electronic mailing list that has superseded bugtraq to become the de facto forum for disseminating advisories, explains {\textquotedbl}We don't believe in security by obscurity, and as far as we know, full disclosure is the only way to ensure that everyone, not just the insiders, have access to the information we need.{\textquotedbl}},
 editor = {Wikipedia},
 year = {2021},
 title = {Full disclosure (computer security)},
 url = {https://en.wikipedia.org/w/index.php?title=Full_disclosure_(computer_security)&oldid=1013071881},
 urldate = {8.2.2022},
 file = {845526c6-2c59-4735-8712-881b4cb449d6:C\:\\Users\\Frantz\\AppData\\Local\\Swiss Academic Software\\Citavi 6\\ProjectCache\\sbu5063berx316u3uwp1jntpx10v7knw016sahglw\\Citavi Attachments\\845526c6-2c59-4735-8712-881b4cb449d6.pdf:pdf}
